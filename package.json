{
  "name": "CheckinRequestService",
  "version": "0.2.1",
  "description": "Lambda for the NYPL Checkin Request Service",
  "main": "index.js",
  "scripts": {
    "pretest": "phplint src/*.php src/**/*.php src/**/**/*.php",
    "test-checkin": "node-lambda run -f config/var_local.env -j config/event_sources_local.json",
    "deploy-development": "./node_modules/.bin/node-lambda deploy -e development -f config/var_development.env -S config/event_sources_development.json -o arn:aws:iam::224280085904:role/lambda_basic_execution --profile nypl-sandbox",
    "deploy-qa": "./node_modules/.bin/node-lambda deploy -e qa -f config/var_qa.env -S config/event_sources_qa.json -o arn:aws:iam::946183545209:role/lambda-full-access --profile nypl-digital-dev",
    "deploy-production": "./node_modules/.bin/node-lambda deploy -e production -f config/var_production.env -S config/event_sources_production.json -o arn:aws:iam::946183545209:role/lambda-full-access --profile nypl-digital-dev"
  },
  "repository": {},
  "keywords": [],
  "dependencies": {},
  "devDependencies": {
    "node-lambda": "0.12.0"
  }
}
